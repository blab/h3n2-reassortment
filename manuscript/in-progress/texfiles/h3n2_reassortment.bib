@article{barr_epidemiological_2010,
    title = {Epidemiological, antigenic and genetic characteristics of seasonal influenza {A}({H}1N1), {A}({H}3N2) and {B} influenza viruses: {Basis} for the {WHO} recommendation on the composition of influenza vaccines for use in the 2009–2010 {Northern} {Hemisphere} season},
